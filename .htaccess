#Options +FollowSymLinks

<IfModule mod_rewrite.c>
    RewriteEngine On

    # Redirect Trailing Slashes...
    #RewriteRule ^(.*)/$ /$1 [L,R=301]

    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-f

    # RewriteRule
    RewriteRule ^ public/index.php [QSA,L]

</IfModule>